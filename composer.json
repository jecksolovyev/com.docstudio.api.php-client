{
    "name": "yuramozol/com.docstudio.api.php-client",
    "description": "DocStudio API Client",
    "type": "library",
    "authors": [
        {
            "name": "Developers Team"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "DocStudio\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "DocStudio\\Client\\" : "test/" }
    }
}
